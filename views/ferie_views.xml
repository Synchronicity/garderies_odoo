<?xml version="1.0"?>
<openerp>
    <record id="view_tree_ferie" model="ir.ui.view">
        <field name="name">Ferie tree</field>
        <field name="model">garderies.ferie</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date_ferie"/>
            </tree>
        </field>
    </record>

    <record id="view_form_ferie" model="ir.ui.view">
        <field name="name">Ferie form</field>
        <field name="model">garderies.ferie</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group name="group_top">
                        <field name="name"/>
                        <field name="date_ferie"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</openerp>
