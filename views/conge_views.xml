<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_tree_conge" model="ir.ui.view">
            <field name="name">Conge tree</field>
            <field name="model">garderies.conge</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="conge_id"/>
                    <field name="debut"/>
                    <field name="fin"/>
                    <field name="type"/>
                </tree>
            </field>
        </record>

        <record id="view_form_conge" model="ir.ui.view">
            <field name="conge_id">Conge form</field>
            <field name="model">garderies.conge</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group name="group_top">
                            <field name="conge_id"/>
                            <field name="debut"/>
                            <field name="fin"/>
                            <field name="type"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</openerp>
